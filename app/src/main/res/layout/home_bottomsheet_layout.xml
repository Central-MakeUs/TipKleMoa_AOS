<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/home_editCat_layout"
        app:layout_behavior="com.google.android.material.bottomsheet.BottomSheetBehavior"
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/dialog_white_roundcorner">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnAllCategory"
            android:layout_marginTop="24dp"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/spoqahansansneo_bold"
            android:text="카테고리 전체"
            android:textColor="@color/black"
            android:textSize="18dp"
            app:layout_constraintStart_toEndOf="@+id/btnAllCategory"
            app:layout_constraintTop_toTopOf="@+id/btnAllCategory" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="24dp"
            android:layout_marginTop="24dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:srcCompat="@drawable/ic_gray_bar" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseClean"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="26dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="청소"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseClean"
            app:layout_constraintStart_toEndOf="@+id/btnChooseClean"
            app:layout_constraintTop_toTopOf="@+id/btnChooseClean" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseCook"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="26dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnChooseClean"
            tools:ignore="SpeakableTextPresentCheck">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="요리"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseCook"
            app:layout_constraintStart_toEndOf="@+id/btnChooseCook"
            app:layout_constraintTop_toTopOf="@+id/btnChooseCook"
            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseBeauty"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="26dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnChooseCook">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="뷰티"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseBeauty"
            app:layout_constraintStart_toEndOf="@+id/btnChooseBeauty"
            app:layout_constraintTop_toTopOf="@+id/btnChooseBeauty"
            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseJachi"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="26dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnChooseBeauty">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="자취"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseJachi"
            app:layout_constraintStart_toEndOf="@+id/btnChooseJachi"
            app:layout_constraintTop_toTopOf="@+id/btnChooseJachi" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseTrip"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginTop="26dp"
            android:layout_marginLeft="24dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnChooseJachi">

        </androidx.appcompat.widget.AppCompatButton>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="여행"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseTrip"
            app:layout_constraintStart_toEndOf="@+id/btnChooseTrip"
            app:layout_constraintTop_toTopOf="@+id/btnChooseTrip"
            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnChooseUniv"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="26dp"
            android:background="@drawable/check_box_line"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnChooseTrip" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosanscjkkr_regular"
            android:includeFontPadding="false"
            android:text="대학"
            android:layout_marginLeft="16dp"
            android:textColor="@color/black"
            android:textSize="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/btnChooseUniv"
            app:layout_constraintStart_toEndOf="@+id/btnChooseUniv"
            app:layout_constraintTop_toTopOf="@+id/btnChooseUniv"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="HardcodedText" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnCompleteEditCategory"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:background="@color/gray"
            android:fontFamily="@font/notosanscjkkr_bold"
            android:includeFontPadding="false"
            android:text="적용"
            android:stateListAnimator="@null"
            android:textColor="@color/white"
            android:textSize="18dp"
            android:layout_marginTop="100dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView14" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.coordinatorlayout.widget.CoordinatorLayout>